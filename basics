10)

import java.util.*;
class MyClass{
    public static void main(String args[]){
        int arr[] = {2,2,9,8,1,9,1};
        int i, j, count, res = 0;
        for(i = 0; i<arr.length; i++){
            count = 0;
            for(j = 0; j<arr.length; j++){
                if(arr[i] == arr[j]) count++;
            }
            if (count != 2) res = arr[i];
        }
        System.out.println(res);
    }
}


