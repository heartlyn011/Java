import java.util.*;
class MyClass{
    public static void main(String args[]){
        int arr[] = {3, 0, 0, 1, 7, 0, 5, 0, 2};
        int i, pos = 0;
        int len = arr.length;
        int temp[] = new int[len];
        for(i = 0; i<arr.length; i++){
            if(arr[i] != 0) {
                temp[pos] = arr[i];
                pos++;
            }
        }
        for(i = 0; i<arr.length;i++) System.out.print(temp[i]+" ");
    }
}
